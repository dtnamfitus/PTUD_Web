{
  "name": "ecommerce",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon --inspect index.js",
    "seed": "node ./seeders/seed.js || exit 0",
    "test": "jest --detectOpenHandles --forceExit",
    "build": "npm run obfuscate && npm run copy",
    "copy-static": "cpy 'public/**/*' dist/public --parents",
    "copy-view": "cpy 'views/**/*' dist/views --parents",
    "obfuscate": "javascript-obfuscator . --output dist/ --exclude node_modules",
    "copy": "cp package.json dist/ && cd dist"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@elastic/elasticsearch": "^8.17.0",
    "axios": "^1.7.8",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "bull": "^4.16.5",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^5.1.0",
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.21.1",
    "express-session": "^1.18.1",
    "joi": "^17.13.3",
    "mongoose": "^8.8.3",
    "mongoose-autopopulate": "^1.1.0",
    "mongoose-elasticsearch-xp": "^5.8.0",
    "mongoose-paginate-v2": "^1.8.5",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.16",
    "nodemon": "^3.1.7",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "qs": "^6.13.1",
    "redis": "^4.7.0",
    "stripe": "^17.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/mongoose-paginate-v2": "^1.6.5",
    "cpy-cli": "^5.0.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.12.0",
    "install": "^0.13.0",
    "javascript-obfuscator": "^4.1.1",
    "jest": "^29.7.0",
    "npm": "^10.9.1",
    "supertest": "^7.0.0",
    "webpack": "^5.96.1",
    "webpack-cli": "^5.1.4"
  }
